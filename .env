# SmartCloudOps AI - Development Environment
# This file is safe for development use only
# Copy and customize for your local environment

ENVIRONMENT=development
DEBUG=false

# Development Prometheus (configure for your local setup)
PROMETHEUS_URL=http://localhost:9090
PROMETHEUS_TIMEOUT=30
PROMETHEUS_SSL_VERIFY=false

# AI Provider Configuration (for development testing)
AI_PROVIDER=fallback
# Uncomment and add your development API keys:
# OPENAI_API_KEY=your-dev-openai-key-here
# GEMINI_API_KEY=your-dev-gemini-key-here

# ML Configuration
ML_MODEL_PATH=ml_models/real_data_model.json
ML_DATA_PATH=data/real_training_data.json
ML_CONFIDENCE_THRESHOLD=0.7
ML_MAX_PREDICTION_TIME=30

# Logging Configuration
LOG_LEVEL=INFO

# Flask Configuration
HOST=0.0.0.0
PORT=5000
WORKERS=1
MAX_CONNECTIONS=100

# CORS for development
CORS_ORIGINS=http://localhost:3000,http://localhost:5000

# Rate limiting for development
RATE_LIMIT=100/hour
