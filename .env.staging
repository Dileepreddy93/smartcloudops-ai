# SmartCloudOps AI - Staging Environment
ENVIRONMENT=staging
DEBUG=false
SECRET_KEY=staging-secure-key

# Staging Infrastructure Configuration  
PROMETHEUS_URL=http://staging-prometheus.internal:9090
PROMETHEUS_TIMEOUT=30
PROMETHEUS_SSL_VERIFY=false

# AI Provider Configuration
AI_PROVIDER=multi
OPENAI_API_KEY_SECRET=smartcloudops-staging/openai/api-key
ANTHROPIC_API_KEY_SECRET=smartcloudops-staging/anthropic/api-key
GOOGLE_API_KEY_SECRET=smartcloudops-staging/google/api-key

# ML Configuration
ML_MODEL_PATH=/app/ml_models/real_data_model.json
ML_DATA_PATH=/app/data/real_training_data.json
ML_CONFIDENCE_THRESHOLD=0.8
ML_MAX_PREDICTION_TIME=20
ML_ENGINE=real_data

# Security Configuration
CORS_ORIGINS=https://staging.smartcloudops.com

# Logging Configuration
LOG_LEVEL=INFO
LOG_FORMAT=json
LOG_FILE=/app/logs/staging.log

# Flask Configuration
HOST=0.0.0.0
PORT=5000
WORKERS=2
MAX_CONNECTIONS=500

# Monitoring Configuration
HEALTH_CHECK_INTERVAL=60
METRICS_ENDPOINT=/metrics
STATUS_ENDPOINT=/status

# Performance Configuration
REQUEST_TIMEOUT=45
MAX_CONTENT_LENGTH=16777216
RATE_LIMIT=500/hour
